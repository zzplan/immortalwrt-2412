#include "rk356x-u-boot.dtsi"

/ {
	chosen {
		stdout-path = &uart2;
	};

	pcat-wdt {
		compatible = "linux,wdt-pcat";
		port = <&uart4>;
		status = "okay";
	};
};

&gpio0 {
	bootph-pre-ram;
};

&sdhci {
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	mmc-hs400-1_8v;
	mmc-hs400-enhanced-strobe;
	pinctrl-0 = <&emmc_bus8 &emmc_clk &emmc_cmd &emmc_datastrobe>;
};

&uart2 {
	clock-frequency = <24000000>;
	bootph-all;
	status = "okay";
};

&vcc3v3_sd {
	bootph-pre-ram;
};
